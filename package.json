{
  "name": "ember-cli-tslint",
  "version": "0.1.4",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember",
    "ember-addon",
    "tslint",
    "tslint ember",
    "tslint ember-cli",
    "lint"
  ],
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/t-sauer/ember-cli-tslint.git"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember serve",
    "test": "mocha node-tests --recursive"
  },
  "dependencies": {
    "broccoli-funnel": "^2.0.1",
    "broccoli-tslinter": "^3.0.1",
    "rsvp": "^4.7.0",
    "tslint": "^5.5.0",
    "walk-sync": "^1.0.0"
  },
  "devDependencies": {
    "@typed-ember/renovate-config": "1.2.1",
    "broccoli-asset-rev": "3.0.0",
    "chai": "4.2.0",
    "ember-cli": "3.22.0",
    "ember-cli-babel": "7.23.0",
    "ember-cli-blueprint-test-helpers": "0.19.2",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-eslint": "5.1.0",
    "ember-cli-htmlbars": "4.4.0",
    "ember-cli-htmlbars-inline-precompile": "3.0.1",
    "ember-cli-inject-live-reload": "2.0.2",
    "ember-cli-qunit": "4.4.0",
    "ember-cli-shims": "1.2.0",
    "ember-cli-sri": "2.1.1",
    "ember-cli-uglify": "3.0.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.1",
    "ember-load-initializers": "2.1.1",
    "ember-resolver": "7.0.0",
    "ember-source": "3.22.0",
    "ember-welcome-page": "4.0.0",
    "loader.js": "4.7.0",
    "mocha": "4.1.0",
    "typescript": "3.4.5"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": "ember-cli-qunit",
    "main": "index.js",
    "defaultBlueprint": "ember-cli-tslint"
  }
}
